# casting = menubah tipe data sebuah variabel 
# jika punya a = int diubah menjadi flot , str,bool

#INTEGER

print("===INTEGER===")
data_int = 222
print("data = ", data_int,"type =", type(data_int))

# ubah int ke float
data_float =float(data_int)

#ubah int ke string
data_float =str(data_int)

#ubah int bool
data_bool =bool(data_int)

# cetak 
print("data = ",data_float,"type ", type(data_float))
print("data = ",data_string, "type(data_string")
print("data = ",data_bool,"type(data_bool)")

print("===float===")
data_float = 222
print("data = ",data_float,"type ", type(data_float))

# ubah float ke int
data_float = int(data_float)

# ubah float ke string
data_str = str(data_string)

# ubah float ke bool
data_bool = bool(data_float)

# cetak
print("data = ",data_float,"type ", type(data_float))
print("data = ",data_str,"type ", type(data_str))
print("data = ",data_bool,"type ", type(data_bool))

print("===str===")
data_str = 222
print("data = ",data_str,"type ", type(data_str))

# ubah str ke int
data_int = int(data_str)

# ubah str ke float
data_float = float(data_str)

# ubah str ke bool
data_bool = bool(data_str)

# cetak
rint("data = ",data_int,"type ", type(data_int))
rint("data = ",data_float,"type ", type(data_float))
rint("data = ",data_str,"type ", type(data_str))

print("===bool===")
data_bool = 222
print("data = ",data_bool,"type ", type(data_bool))

# ubah bool ke int
data_int = int(data_bool)

# ubah bool ke float
data_float = float(data_bool)

# ubah bool ke str
data_str = str(data_bool)

# cetak bool ke str
rint("data = ",data_int,"type ", type(data_int))
rint("data = ",data_float,"type ", type(data_float))
rint("data = ",data_str,"type ", type(data_str))
